{
  "name": "guld-env",
  "version": "1.0.14",
  "description": "Guld environment detection module.",
  "main": "index.js",
  "browser": "guld-env.min.js",
  "scripts": {
    "test": "npm run test:browser && npm run test:node",
    "test:browser": "mochify test/browser.js --url file://$PWD/test/test.html",
    "test:node": "mocha test/node.js",
    "lint": "eslint --env browser *.js* test/*.js",
    "fix": "eslint --fix *.js* test/*.js",
    "build": "webpack-cli"
  },
  "keywords": [
    "guld",
    "cli",
    "OS detection",
    "browser detection",
    "distro detection",
    "environment"
  ],
  "author": "isysd",
  "license": "MIT",
  "dependencies": {
    "detect-browser": "^3.0.0",
    "getos": "^3.1.0",
    "pify": "^3.0.0",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "readme": "README.md",
  "engines": {
    "node": ">=7"
  },
  "homepage": "https://bitbucket.org/isysd/tech-js-node_modules-guld-env#readme",
  "repository": "git@bitbucket.org:isysd/tech-js-node_modules-guld-env.git"
}
